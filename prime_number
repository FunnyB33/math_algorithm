number = int(input("数字を入力してください: "))
heihoukon = int(number**(1/2))
heihoukon_list = []

for i in range(2, heihoukon + 1):
	heihoukon_list.append(i)

is_prime = True
for k in heihoukon_list:
	amari = number % k
	if amari == 0:
		is_prime = False
		break

if is_prime and number > 1:
	print(str(number) + ' は素数です!')
else:
	print(str(number) + ' は素数ではありません。')
